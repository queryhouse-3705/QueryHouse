SELECT t1.c0 FROM t1 WHERE LOWER(t1.c0) = 'v0 full-text search';
SELECT t2.c0 FROM t2 WHERE t2.c0 = 'v0 multiple spaces';
SELECT t1.c0 FROM t1 WHERE t1.c0 LIKE '%replace%' ORDER BY t1.c0 DESC;
SELECT t2.c0 FROM t2 WHERE t2.c0 LIKE '%ghi%' LIMIT 1;
SELECT t0.c0 FROM t0 WHERE INSTR(t0.c0, 'example') = 1;
SELECT t2.c0 FROM t2 WHERE TRIM(t2.c0) = 'v1 no space';
SELECT t1.c0 FROM t1 WHERE t1.c0 NOT LIKE 'v0%replace';
SELECT t0.c0 FROM t0 WHERE LOWER(REPLACE(t0.c0, 'quick', 'fast')) = 'v0 fast brown fox';
SELECT t0.c0 FROM t0 WHERE t0.c0 LIKE '%apple%' OR t0.c0 LIKE '%banana%';
SELECT t1.c0 FROM t1 WHERE t1.c0 LIKE 'v0 %place%';
SELECT t2.c0 FROM t2 WHERE REPLACE(t2.c0, 'no', 'yes') = 'v1 yes space';
SELECT t1.c0, t2.c0 FROM t1, t2 WHERE t1.c0 LIKE '%replace%' AND t2.c0 LIKE '%ghi%';
SELECT t0.c0 FROM t0 WHERE t0.c0 || ' test' = 'v0 example test';
SELECT t2.c0 FROM t2 WHERE t2.c0 NOT LIKE '%space%';
SELECT t0.c0 FROM t0 WHERE t0.c0 NOT IN ('v0 lazy dog', 'v0 apple');
SELECT t2.c0 FROM t2 WHERE INSTR(t2.c0, 'locate') > 5;
SELECT t1.c0 FROM t1 WHERE t1.c0 NOT IN ('v0 database indexing', 'v0 replace');
SELECT t0.c0, t1.c0 FROM t0, t1 WHERE t0.c0 LIKE '%quick%' AND t1.c0 LIKE '%replace%';
SELECT t2.c0 FROM t2 WHERE t2.c0 LIKE '%no%';
SELECT t0.c0 FROM t0 WHERE t0.c0 NOT LIKE 'v0%example';
