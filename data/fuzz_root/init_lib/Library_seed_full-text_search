SELECT t0.c0 FROM t0 WHERE REPLACE(t0.c0, 'quick', 'fast') = 'v0 fast brown fox';
SELECT t1.c0 FROM t1 WHERE NOT t1.c0 LIKE '%search%';
SELECT t2.c0 FROM t2 WHERE t2.c0 LIKE '%no space%';
SELECT t0.c0, t2.c0 FROM t0, t2 WHERE t0.c0 = 'v0 lazy dog' AND t2.c0 = 'v0 multiple spaces';
SELECT t1.c0 FROM t1 WHERE t1.c0 IN ('v0 replace this', 'v0 replace that');
SELECT t2.c0 FROM t2 WHERE t2.c0 LIKE '%abc%' AND NOT t2.c0 LIKE '%def%';
SELECT t0.c0, t1.c0 FROM t0, t1 WHERE NOT t0.c0 LIKE '%banana%' AND t1.c0 LIKE '%replace%';
SELECT t2.c0 FROM t2 WHERE t2.c0 IS NULL OR t2.c0 = 'v0 spaces';
SELECT t1.c0 FROM t1 WHERE NOT t1.c0 IN ('v0 database indexing', 'v0 replace');
SELECT t0.c0 FROM t0 WHERE t0.c0 = 'v0 example' AND t0.c0 <> 'v0 lazy dog';
SELECT t1.c0, t2.c0 FROM t1, t2 WHERE LOWER(t1.c0) = LOWER(t2.c0);
SELECT t2.c0 FROM t2 WHERE t2.c0 LIKE '%ghi%' LIMIT 3;
SELECT t0.c0 FROM t0 WHERE t0.c0 LIKE 'v0%' ORDER BY t0.c0 ASC;
SELECT t1.c0 FROM t1 WHERE t1.c0 LIKE '%replace%' AND t1.c0 LIKE '%find%';
SELECT t2.c0 FROM t2 WHERE t2.c0 LIKE 'v0 spaces' AND INSTR(t2.c0, 'spaces') > 0;
SELECT t0.c0 FROM t0 WHERE INSTR(t0.c0, 'quick') > 0 AND NOT t0.c0 LIKE '%lazy%';
SELECT t1.c0 FROM t1 WHERE t1.c0 = 'v0 full-text search' OR t1.c0 LIKE '%indexing%';
SELECT t0.c0 FROM t0 WHERE t0.c0 = 'v0 apple' AND NOT t0.c0 IN ('v0 banana', 'v0 cherry');
SELECT t1.c0, t2.c0 FROM t1, t2 WHERE t1.c0 LIKE '%database%' AND t2.c0 LIKE '%abc%';
SELECT t0.c0 FROM t0 WHERE t0.c0 LIKE 'v0 example%' AND t0.c0 <> 'v0 quick brown fox';
SELECT t2.c0 FROM t2 WHERE INSTR(t2.c0, 'no') = 1 OR INSTR(t2.c0, 'spaces') = 5;
SELECT t0.c0 FROM t0 WHERE t0.c0 LIKE '%quick%' AND NOT t0.c0 LIKE '%lazy%';
SELECT t0.c0 FROM t0 WHERE t0.c0 IN (SELECT t0.c0 FROM t0 WHERE t0.c0 LIKE '%apple%');
SELECT t1.c0 FROM t1 WHERE NOT t1.c0 IN (SELECT t1.c0 FROM t1 WHERE t1.c0 LIKE '%database%');
SELECT t0.c0, t1.c0 FROM t0, t1 WHERE t0.c0 = t1.c0 || ' quick';
SELECT t2.c0 FROM t2 WHERE t2.c0 = SUBSTRING(REPLACE(t2.c0, 'ghi', 'xyz'), 1, 5);
SELECT t1.c0, t2.c0 FROM t1, t2 WHERE TRIM(t1.c0) = TRIM(t2.c0) AND t2.c0 LIKE '%locate%';
SELECT t2.c0 FROM t2 WHERE REPLACE(REPLACE(t2.c0, 'multiple', 'many'), 'spaces', 'gaps') = 'v0 many gaps';
SELECT t1.c0 FROM t1 WHERE REPLACE(LOWER(t1.c0), 'replace', 'updated') = 'v0 updated this';
