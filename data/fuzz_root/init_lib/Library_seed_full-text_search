SELECT t2.c0 FROM t2 WHERE t2.c0 LIKE '%locate%' AND t2.c0 LIKE '%spaces%';
SELECT t0.c0 FROM t0 WHERE t0.c0 LIKE 'v0 quick%';
SELECT t1.c0 FROM t1 WHERE t1.c0 = 'v0 database indexing';
SELECT t2.c0 FROM t2 WHERE NOT t2.c0 IS NULL;
SELECT t0.c0 FROM t0 WHERE t0.c0 LIKE '%lazy%' AND NOT t0.c0 LIKE '%dog%';
SELECT t1.c0 FROM t1 WHERE INSTR(t1.c0, 'replace') > 0;
SELECT t2.c0 FROM t2 WHERE REPLACE(t2.c0, 'ghi', 'xyz') = 'v1 xyz';
SELECT t0.c0 FROM t0 WHERE t0.c0 IN ('v0 example', 'v0 quick brown fox');
SELECT t1.c0, t2.c0 FROM t1, t2 WHERE t1.c0 LIKE '%find%' AND NOT t2.c0 LIKE '%locate%';
SELECT t2.c0 FROM t2 WHERE t2.c0 LIKE '%multiple%' AND t2.c0 LIKE '%spaces%';
SELECT t0.c0 FROM t0 WHERE SUBSTRING(t0.c0, 1, 10) = 'v0 quick ';
SELECT t1.c0 FROM t1 WHERE TRIM(t1.c0) = 'v0 find and replace';
SELECT t0.c0 FROM t0 WHERE LOWER(t0.c0) = 'v0 quick brown fox';
